{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"blockTypes": ["core/group"],
	"slug": "section-contrast",
	"title": "[Contrast]",
	"styles": {
		"color": {
			"background": "var:preset|color|contrast-2",
			"text": "var:preset|color|base-2"
		},
		"elements": {
			"heading":{
				"color": {
					"text": "var:preset|color|base"
				}
			},
			"link":{
				"color": {
					"text": "var:preset|color|base-5"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover":{
					"color": {
						"text": "var:preset|color|base"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		}
	}
}
