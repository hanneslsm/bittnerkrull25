{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"blockTypes": ["core/group"],
	"slug": "section-contrast-2",
	"title": "[Contrast-2]",
	"styles": {
		"color": {
			"background": "var:preset|color|contrast-2",
			"text": "var:preset|color|base-2"
		},
		"elements": {
			"heading":{
				"color": {
					"text": "var:preset|color|base"
				}
			},
			"link":{
				"color": {
					"text": "var:preset|color|brand-4"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover":{
					"color": {
						"text": "var:preset|color|base"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|base-2",
					"text": "var:preset|color|contrast-3"
				},
				"css": ".wp-block-button__link:not(.has-background):hover {background-color:color-mix(in srgb, var(--wp--preset--color--base-2) 90%, transparent);}",
				"variations": {
					"outline": {
						"css": ".wp-block-button__link:not(.has-background):hover {background-color:color-mix(in srgb, var(--wp--preset--color--base-2) 5%, transparent);}"
					}
				}
			}
		}
	}
}
